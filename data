эталонный размер популяции                                N = 1000
количество популяций                                      k = 2
кол-ва образцов в каждой популяции                        n = 3 1
время слияния всех популяций                              t = 10000
частота миграции(матрица k на k, диагональ - нулевая)     m = 0 2 1 0
частота коалесценции(матрица 1 на k)                      q = 1 1
частота коалесценции в "объединённой" популяции           Q = 1
дерево (для первого варианта, но само дерево пока не задействовано вообще) T = (2:25,(0:10,1:10):15);
